#!/bin/bash
realpath=`readlink -fn $0`
export RPM_USRLIBRPM=/home/daniel/Schreibtisch/Git/Zybo-Linux/Yocto/poky/build/tmp/sysroots/x86_64-linux/usr/lib/rpm RPM_ETCRPM=/home/daniel/Schreibtisch/Git/Zybo-Linux/Yocto/poky/build/tmp/sysroots/x86_64-linux/etc/rpm RPM_LOCALEDIRRPM=/home/daniel/Schreibtisch/Git/Zybo-Linux/Yocto/poky/build/tmp/sysroots/x86_64-linux/usr/share/locale
exec -a `dirname $realpath`/rpmresolve `dirname $realpath`/rpmresolve.real "$@"
